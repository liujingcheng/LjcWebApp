@{
    ViewData["Title"] = "New Page";
}
@model LjcWebApp.Models.ViewModels.UploadModel
<form asp-controller="Default" role="form" asp-action="Upload" enctype="multipart/form-data" method="post">
    <div class="form-group">
        <label asp-for="UploadedFile"></label>
        <input type="file" asp-for="UploadedFile" />
        <p class="help-block">上传</p>
    </div>
    <button type="submit" class="btn btn-default">提交</button>
</form>

